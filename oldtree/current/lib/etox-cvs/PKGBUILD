# Maintainer: Amar Hadzihasanovic <rensel[at]email.it>

pkgname=etox-cvs
pkgver=20041109
pkgrel=1
pkgdesc="An Enlightened Layout Library"
url="http://www.enlightenment.org"
groups=('efl')
depends=('openssl' 'evas' 'ecore' 'eet')
makedepends=('cvs')
provides=('etox')
replaces=('estyle-cvs')
source=()

build() {
  cvs -z3 -d:pserver:anonymous@cvs.sourceforge.net:/cvsroot/enlightenment co -D "2004-11-09" e17/libs/etox
  cd $startdir/src/e17/libs/etox
  ./autogen.sh --prefix=/opt/efl
  make || return 1
  make DESTDIR=$startdir/pkg install
}
