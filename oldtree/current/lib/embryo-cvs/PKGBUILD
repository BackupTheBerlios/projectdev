# Maintainer: Amar Hadzihasanovic <rensel[at]email.it>

pkgname=embryo-cvs
pkgver=20041109
pkgrel=1
pkgdesc="EDJE Virtual Machine"
groups=('efl')
url="http://www.enlightenment.org"
depends=()
makedepends=('cvs')
provides=('embryo')
source=()

build() {
  cvs -z3 -d:pserver:anonymous@cvs.sourceforge.net:/cvsroot/enlightenment co -D "2004-11-09" e17/libs/embryo
  cd $startdir/src/e17/libs/embryo
  ./autogen.sh --prefix=/opt/efl
  make || return 1
  make DESTDIR=$startdir/pkg install
}
